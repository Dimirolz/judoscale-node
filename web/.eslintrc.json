{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "root": true,
    "rules": {
      "semi": [2, "never"],
      "comma-dangle": [2, "never"],
      "no-console": 0
    }
}
