{
  "release-type": "node",
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "packages",
      "components": [
        "packages/node-core", "packages/express", "packages/fastify", "packages/bullmq"
      ]
    }
  ],
  "packages": {
    "packages/node-core": {
      "release-type": "node"
    },
    "packages/express": {
      "release-type": "node"
    },
    "packages/fastify": {
      "release-type": "node"
    },
    "packages/bullmq": {
      "release-type": "node"
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
